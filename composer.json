{
    "name": "devuniverse/laravel-gettext",
    "description": "Adds localization support to laravel applications in an easy way using Poedit and GNU gettext.",
    "homepage": "https://github.com/devuniverse/laravel-gettext",
    "keywords": [
        "gettext",
        "localization",
        "poedit",
        "laravel-gettext",
        "laravel", "translation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "NicolÃ¡s Daniel Palumbo",
            "email": "n@xinax.net"
        },
        {
            "name": "Antoine Aflalo",
            "email": "dev+laravel_gettext@zerospam.ca"
        },
        {
            "name": "Jay",
            "email": "ishimwe@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/devuniverse/laravel-gettext/issues"
    },
    "require": {
        "php": ">=7.3",
        "laravel/framework": "^6.0 ||^7.0 ||^8.0 ||^9.0||^10.0||^11.0",
        "laravel/helpers": "^1.1"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "phpunit/phpunit": "~7.0 || ^8.0|| ^9.0|| ^10.0||^11.0",
        "squizlabs/php_codesniffer" : "1.5.*",
        "laravel/laravel": "^6.0 ||^7.0 ||^8.0 ||^9.0 ||^10.0||^11.0",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-0": {
            "Xinax\\LaravelGettext\\": "src/"
        },
        "files": [
            "src/Xinax/LaravelGettext/Support/helpers.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
          "providers": [
            "Xinax\\LaravelGettext\\LaravelGettextServiceProvider"
          ]
       }
    }
}
